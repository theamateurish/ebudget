	<queryString>
		<![CDATA[select * from temp.temp_FPP($P{legal},$P{fund}) t1 FULL OUTER JOIN (select split_part(fpp_codes, '-',1)as fpp_code,sum(amnt) FROM
		(select * from temp.temp_FPP($P{legal},$P{fund}) where classes>0) total_fund
group by split_part(fpp_codes, '-',1)

union

select split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2) as fpp_code,sum(amnt) FROM
		(select * from temp.temp_FPP($P{legal},$P{fund}) where classes>0) total_services
group by split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)

union

select split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3) as fpp_code,sum(amnt) FROM
		(select * from temp.temp_FPP($P{legal},$P{fund}) where classes>0) total_ofis
group by split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3)

union

select split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3)||'-'||
	split_part(fpp_codes, '-',4) as fpp_code,sum(amnt) FROM
		(select * from temp.temp_FPP($P{legal},$P{fund}) where classes>0) total_subofis
group by split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3)||'-'||
	split_part(fpp_codes, '-',4)

union

select split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3)||'-'||
	split_part(fpp_codes, '-',4)||'-'||
	split_part(fpp_codes, '-',5) as fpp_code,sum(amnt) FROM
		(select * from temp.temp_FPP($P{legal},$P{fund}) where classes>0) total_programs
group by split_part(fpp_codes, '-',1)||'-'||
	split_part(fpp_codes, '-',2)||'-'||
	split_part(fpp_codes, '-',3)||'-'||
	split_part(fpp_codes, '-',4)||'-'||
	split_part(fpp_codes, '-',5)

union

select fpp_codes,sum(amnt) from temp.temp_FPP($P{legal},$P{fund}) where classes>0
group by fpp_codes) t2 on t1.fpp_codes=t2.fpp_code
order by t1.fpp_codes]]>
	</queryString>
